#include<iostream.h>
#include<conio.h>
int func(int);
void main()
{
clrscr();
int num,i;
cout<<"Enter a number: ";
cin>>num;
for(i=2;i<=num/2;i++)
 {
  if(func(i))
   {
    if(func(num-i)
     cout<<num<<"="<<i<<"+"<<num-i<<endl;
   }
  }
getch();
}
int func(int num)
 {
  int j;
  int flag=1;
  for(j=2;j<=num/2;j++)
   {
    if(num%j==0)
     {
      flag=0;
      break;
     }
    return flag;
 }
 
    
   
